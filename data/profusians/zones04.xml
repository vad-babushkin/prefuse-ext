<zone shape="rectangular" name="green zone" shapedata="100,-100,120,120"  itemcolor="0,200,0" fillcolor="0,200,0,111"></zone>
<zone shape="rectangular" name="purple zone" shapedata="-100,100,110,90"  itemcolor="100,0,100" fillcolor="100,0,100,111"></zone>


</zonexml>
